<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iotek.ssm.dao.AttendanceDao">
  <insert id="insertAttendance"  parameterType="Attendance">
    insert into atten (aid,eid,data,checkin,checkout,latetime,overtime)
     values (#{aid},#{empl.id},#{data},#{checkIn},#{checkOut},#{lateTime},#{overTime})
  </insert>
    
</mapper>