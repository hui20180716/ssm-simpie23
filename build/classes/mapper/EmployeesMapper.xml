<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iotek.ssm.dao.EmployeesDao">
    <select id="findAllEmployees" resultType="Employees">
	   select  * from Employees
	</select>
	
	<select id="findEmployeesByName" resultType="Employees" >
	   select  * from Employees  where  name = (#{name})
	</select>
	
	<insert id="insertEmployees">
	    insert into Employees (name,password,createtime,did,pid,sid,aid,rid,tid,said,oid,status) 
	    values (#{name},#{password},#{createTime},#{dept.dId},#{posi.pId},#{salary.sId},
	    #{atten.aid},#{resume.rid},#{train.tId},#{salaryAbout.saId},#{overtime.oId},#{status});                                                        
	</insert>
</mapper>